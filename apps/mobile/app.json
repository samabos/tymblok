{
  "expo": {
    "name": "Tymblok",
    "slug": "tymblok",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "tymblok",
    "userInterfaceStyle": "automatic",
    "splash": {
      "backgroundColor": "#6366F1"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.tymblok.app"
    },
    "android": {
      "package": "io.tymblok.app",
      "usesCleartextTraffic": true
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
